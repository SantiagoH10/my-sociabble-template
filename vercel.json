{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "devCommand": null,
  "buildCommand": null,
  "outputDirectory": "dist",
  "functions": {
    "api/*.js": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ]
}
